{
  "name": "parking-server",
  "version": "0.0.1",
  "description": "A simple app for parking management",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "build": "cross-env NODE_ENV=production tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "cross-env eslint . --ext .js,.jsx,.ts,.tsx",
    "fmt": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "cross-env NODE_ENV=testing jest",
    "clean": "rimraf ./build"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.5",
    "morgan": "^1.10.0",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novanet-Studio/parking.git"
  },
  "keywords": [],
